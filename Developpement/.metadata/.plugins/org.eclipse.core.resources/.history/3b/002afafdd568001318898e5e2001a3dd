package Test;

import java.util.Vector;
import modele.*;


// Ce test nous permet de vérifier la fonction mesUrl() 
// qui renvoie la liste des liens contenu dans un fichier 
// de type texte ou html, le traitement est le même dans les deux cas.
public class UseOtherTest {
	
	
	public static void main(String[] args) {
		UseOther montxt= new UseOther("test.txt");

		Vector<String> vUrl = (montxt.mesUrl());
		
		int i=0;
		int nbEl = vUrl.size();
		
		
		System.out.println("Test d'un fichier txt.");
		System.out.println();
		while (i<nbEl){
			System.out.println(vUrl.elementAt(i));
			i++;
		}
		
		
		
		
		
		
		UseOther monhtml= new UseOther("test.txt");

		Vector<String> vUrl2 = (monhtml.mesUrl());
		
		int i=0;
		int nbEl = vUrl.size();
		
		
		System.out.println("Test d'un fichier txt.");
		System.out.println();
		while (i<nbEl){
			System.out.println(vUrl.elementAt(i));
			i++;
		}
	}
}
