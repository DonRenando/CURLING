import java.io.File;
import java.io.IOException;

import java.util.Scanner;


	public class TestRegex {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		try {
		Scanner monScanner = new Scanner(new File("test.txt"));
		String expression = "^[a-zA-Z0-9\-\.]+\.(com|org|net|mil|edu|COM|ORG|NET|MIL|EDU)$";
		monScanner.findInLine(expression);
		
		while (monScanner.hasNext())
			//System.out.println(monScanner.next());
			;
		
		} catch(IOException ignore){}
	}

}
