package Test;

import java.util.Vector;
import modele.*;


// Ce test nous permet de vérifier la fonction mesUrl() qui renvoie la liste des liens contenu dans ce fichier "document.xml"
public class UseOtherTest {
	
	
	public static void main(String[] args) {
		UseOther mondocx= new UseOther("test.txt");

		Vector<String> vUrl = (mondocx.mesUrl());
		
		int i=0;
		int nbEl = vUrl.size();
		
		
		System.out.println("Test d'un fichier txt.");
		while (i<nbEl){
			System.out.println(vUrl.elementAt(i));
			i++;
		}
	}
}
