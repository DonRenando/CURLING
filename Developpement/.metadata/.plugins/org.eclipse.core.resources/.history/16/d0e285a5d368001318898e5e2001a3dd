package Test;

import modele.*;

import java.io.File;
import java.io.IOException;
import java.io.InputStream;
import java.util.Enumeration;
import java.util.Vector;
import java.util.zip.ZipEntry;
import java.util.zip.ZipFile;

import java.util.Scanner;


// Ce test nous permet de vérifier à la fois la fonction getZipFileStream qui permet de retourner 
// l'entré du fichier "document.xml" contenu dans l'archive ".docx" ainsi que de tester la fonction
// mesUrl() qui renvoie la liste des liens contenu dans ce fichier "document.xml"
public class ZipFileTest {
	
	
	public static void main(String[] args) {
		UseDocx mondocx= new UseDocx("test.docx");

		Vector<String> vUrl = (new ZipFileTest()).(mondocx.mesUrl()));
		
		int i=0;
		int nbEl = vUrl.size();
		
		while (i<nbEl){
			System.out.println(vUrl.elementAt(i));
			i++;
		}
	}
}
